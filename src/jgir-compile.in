#!/usr/bin/python
import os,sys,subprocess

classpath="@CLASSPATH@"
args = ['java', '-cp', classpath, 'org.gnome.gir.compiler.CodeFactory']
args.extend(sys.argv[1:])
subprocess.check_call(args,
                      stdout=sys.stdout,
                      stderr=sys.stderr,
                      close_fds=True)
